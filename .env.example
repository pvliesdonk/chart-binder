MUSICBRAINZ_WEB_URL=""

# LangSmith (optional - tracing/observability)
LANGSMITH_TRACING="false"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=""
LANGSMITH_PROJECT="chart-binder"

# Local Services (optional)
